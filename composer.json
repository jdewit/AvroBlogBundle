{
    "name": "avro/blog-bundle",
    "type": "symfony-bundle",
    "description": "Another very simple Symfony2 Blog bundle",
    "keywords": ["blog"],
    "homepage": "http://github.com/jdewit/AvroBlogBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Joris de Wit",
            "email": "joris.w.dewit@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "knplabs/knp-paginator-bundle": "*",
        "doctrine/doctrine-bundle": "*",
        "stof/doctrine-extensions-bundle": "*"
    },
    "suggest": {
        "doctrine/orm": "If you want to use orm for storage",
        "doctrine/mongodb-odm-bundle": "For mongodb storage"
    },
    "autoload": {
        "psr-0": { "Avro\\BlogBundle": "" }
    },
    "target-dir": "Avro/BlogBundle"
}
